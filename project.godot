; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Project Recordkeeper"
run/main_scene="res://default-scene.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
config/icon="res://icon.svg"

[aseprite_importers]

aseprite_executable_path="A:/Games/SteamLibrary/steamapps/common/Aseprite/Aseprite.exe"

[autoload]

tree_accessor="*res://0_scripts/singletons/tree_accessor.gd"
SoundManager="*res://addons/sound_manager/sound_manager.gd"

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor]

naming/node_name_num_separator=2
naming/node_name_casing=2
naming/default_signal_callback_name="on_{node_name}_{signal_name}"
naming/default_signal_callback_to_self_name="on_{signal_name}"

[editor_plugins]

enabled=PackedStringArray("res://addons/gut/plugin.cfg", "res://addons/nklbdev.aseprite_importers/plugin.cfg", "res://addons/sound_manager/plugin.cfg")

